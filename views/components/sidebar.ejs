  <a href="/dashboard" style="text-decoration: none; cursor: pointer">
    <h1 class="title">HappyMe</h1>
  </a>
  <div class="profile">
    <img src="./images/btlogo.png" alt="" />
    <a href="/dashboard/profile" style="text-decoration: none"
      ><div class="profile-content">
        <h2 class="user-name"><%= user.name %></h2>
        <p class="user-original"><%= user.username %></p>
      </div>
    </a>
  </div>
  <ul class="sidebar-links">
    <a href="/dashboard/excercises" class="link">
      <li>
        <span class="icons">
          <img
            src="data:image/png;base64,"
          />
        </span>
        <p class="name-link">Exercises</p>
      </li></a
    >
    <a href="/dashboard/tasks" class="link">
      <li>
        <span class="icons">
          <img
            src="data:image/png;base64,"
          />
        </span>
        <p class="name-link">Task</p>
      </li></a
    >
    <a href="/dashboard/therapist" class="link">
      <li>
        <span class="icons">
          <img
            src="data:image/png;base64,"
          />
        </span>
        <p class="name-link">Therapists</p>
      </li></a
    >
  </ul>
  <a href="/logout" style="text-decoration: none">
    <div class="logout-link">
      <img src="" alt="" />
      <p class="name-link">Log Out</p>
    </div>
  </a>
</div>
